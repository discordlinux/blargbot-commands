{//;get ddg results}

{set;!~rawresults;{jget;{base64decode;{jget;{request;https://api.duckduckgo.com/?q={uriencode;! {params}}&format=json&pretty=1&no_html=1&skip_disambig=1&no_redirect=1};body}};Redirect}}
